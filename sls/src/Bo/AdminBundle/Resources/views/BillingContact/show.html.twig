    <table>
        <tbody>
            <tr>
                <th width="100px">Id:</th>
                <td>{{ billingContact.id }}</td>
            </tr>
			<tr><td>&nbsp;</td></tr>
            <tr>
                <th>Name:</th>
                <td>{{ billingContact.name }}</td>
            </tr>
			<tr><td>&nbsp;</td></tr>
            <tr>
                <th>{{'label.email'|trans([],'BoAdminBundle')}}:</th>
                <td>{{ billingContact.email }}</td>
            </tr>
			<tr><td>&nbsp;</td></tr>
            <tr>
                <th>Phone:</th>
                <td>{{ billingContact.phone }}</td>
            </tr>
			<tr><td>&nbsp;</td></tr>
            <tr>
                <th>{{'label.creationdate'|trans([],'BoAdminBundle')}}:</th>
                <td>{% if billingContact.creationdate %}{{ billingContact.creationdate|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
			<tr><td>&nbsp;</td></tr>
        </tbody>
    </table>
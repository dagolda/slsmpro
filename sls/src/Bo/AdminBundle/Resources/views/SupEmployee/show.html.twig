    <table width="100%">
        <tbody>
            <tr>
                <th width="24%">{{'label.sex'|trans([],'BoAdminBundle')}}</th>
				<th width="24%">{{'label.name'|trans([],'BoAdminBundle')}}</th>
				<th width="24%">{{'label.firstname'|trans([],'BoAdminBundle')}}</th>
				<th width="24%">{{'label.email'|trans([],'BoAdminBundle')}}</th>
            </tr>
            <tr>
                <td>{{ supemployee.sexe }}</td>
                <td>{{ supemployee.name }}</td>
				<td>{{ supemployee.firstname }}</td>
				<td>{{ supemployee.email }}</td>
            </tr>
            <tr><td colspan="4">&nbsp;</td></tr>
            <tr>
				<th>{{'label.status'|trans([],'BoAdminBundle')}}</th>
				<th>Qualification</th>
				<th>{{'label.language'|trans([],'BoAdminBundle')}}</th>
				<th>{{'label.englishlevel'|trans([],'BoAdminBundle')}}</th>
            </tr>
            <tr>
                <td>{{ supemployee.status }}</td>
				<td>
					{%if supemployee.typecontract %}
						{% set i=0 %}
						{%for se in supemployee.typecontract %}
							{% set i=i+1 %}
							{{ se.reference }}{% if i<supemployee.typecontract|length %}, {% endif %}
						{% endfor %}
					{% else %}
						---
					{% endif %}
				</td>
				<td>{%if supemployee.language %}{{ supemployee.language }}{%else%}---{% endif %}</td>
				<td>{%if supemployee.englishlevel %}{{ supemployee.englishlevel }}{%else%}---{% endif %}</td>
            </tr>
            <tr><td colspan="4">&nbsp;</td></tr>
            <tr>
                <th>{{'label.homenumber'|trans([],'BoAdminBundle')}}</th>
				<th>{{'label.cellnumber'|trans([],'BoAdminBundle')}}</th>
                <th>{{'label.address'|trans([],'BoAdminBundle')}} 1</th>
				<th>{{'label.address'|trans([],'BoAdminBundle')}} 2</th>
            </tr>
            <tr>
                <td>{%if supemployee.homenumber %}{{ supemployee.homenumber }}{%else%}---{% endif %}</td>
                <td>{%if supemployee.cellnumber %}{{ supemployee.cellnumber }}{%else%}---{% endif %}</td>
                <td>{%if supemployee.address1 %}{{ supemployee.address1 }}{%else%}---{% endif %}</td>
                <td>{%if supemployee.address2 %}{{ supemployee.address2 }}{%else%}---{% endif %}</td>
            </tr>
			<tr><td colspan="4">&nbsp;</td></tr>
            <tr>               
				<th>{{'label.category'|trans([],'BoAdminBundle')}}</th>
                <th>{{'label.birthdate'|trans([],'BoAdminBundle')}}</th>
                <th>{{'label.hiringdate'|trans([],'BoAdminBundle')}}</th>
				<th>{{'label.creationdate'|trans([],'BoAdminBundle')}}</th>
            </tr>
            <tr>
				<td>{%if supemployee.categoryteacher %}{{ supemployee.categoryteacher }}{%else%}---{% endif %}</td>
                <td>{% if supemployee.birthdate %}{{ supemployee.birthdate|date('Y-m-d') }}{%else%}---{% endif %}</td>
                <td>{% if supemployee.hiringdate %}{{ supemployee.hiringdate|date('Y-m-d') }}{%else%}---{% endif %}</td>
				<td>{% if supemployee.creationdate %}{{ supemployee.creationdate|date('Y-m-d H:i:s') }}{%else%}---{% endif %}</td> 				
            </tr>
			<tr><td colspan="4">&nbsp;</td></tr>
            <tr>               
				<th>{{'label.creationdate'|trans([],'BoAdminBundle')}}</th>
				<th>{{'label.deleteby'|trans([],'BoAdminBundle')}}</th>
                <th>&nbsp;</th>
                <th>&nbsp;</th>
            </tr>
            <tr>
				<td>{% if supemployee.deleteddate %}{{ supemployee.deleteddate|date('Y-m-d H:i:s') }}{%else%}---{% endif %}</td>
				<td>{%if supemployee.deleteby %}{{ supemployee.deleteby }}{%else%}---{% endif %}</td>
                <td>&nbsp;</td>
                <td>&nbsp;</td>				
            </tr>
			<tr><td colspan="4">&nbsp;</td></tr>
        </tbody>
    </table>
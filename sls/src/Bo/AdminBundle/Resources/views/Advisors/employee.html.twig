    <table width="100%">
        <tbody>
            <tr>
                <th width="18%">{{'label.id'|trans([],'BoAdminBundle')}}</th>
				<th width="18%">{{'label.name'|trans([],'BoAdminBundle')}}</th>
				<th width="18%">{{'label.firstname'|trans([],'BoAdminBundle')}}</th>
				<th width="18%">{{'label.email'|trans([],'BoAdminBundle')}}</th>
				<th width="18%">{{'label.language'|trans([],'BoAdminBundle')}}</th>
            </tr>
            <tr>
                <td>{{ employee.id }}</td>
                <td>{{ employee.name }}</td>
				<td>{{ employee.firstname }}</td>
				<td>{{ employee.email }}</td>
				<td>{{ employee.langage }}</td>
            </tr>
            <tr><td colspan="4">&nbsp;</td></tr>
            <tr>
				<th>{{'label.status'|trans([],'BoAdminBundle')}}</th>
				<th>Qualification</th>
                <th>{{'label.homenumber'|trans([],'BoAdminBundle')}}</th>
				<th>{{'label.cellnumber'|trans([],'BoAdminBundle')}}</th>				
            </tr>
            <tr>
                <td>{{ employee.status }}</td>
                <td>
					{%set i=0%}
					{%for tc in employee.typecontract%}
						{{ tc.reference }}
						{%set i=i+1%}
						{%if i<employee.typecontract|length%}-{%endif%}
					{%endfor%}
				</td>			
                <td>{%if employee.homenumber %}{{ employee.homenumber }}{%else%}---{% endif %}</td>
                <td>{%if employee.cellnumber %}{{ employee.cellnumber }}{%else%}---{% endif %}</td>				
            </tr>
            <tr><td colspan="4">&nbsp;</td></tr>
        </tbody>
    </table>
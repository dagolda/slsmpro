		<tr><td colspan="3">&nbsp;</td></tr>
		<tr>
			<th colspan="3" class="sty_label">{{'label.placeoftraining'|trans([],'BoAdminBundle')}}</th>
		</tr>
		<tr>
			<td colspan="3">{{ form_widget(form.placeoftraining, {'attr' : {'class' : 'placeoftraining span3'}}) }}</td>		
		</tr>
		<tr><td colspan="3">&nbsp;</td></tr>
		<tr>
			<th class="sty_label" colspan="3">{{'label.comment'|trans([],'BoAdminBundle')}}</th>
		</tr>
		<tr>
			<td colspan="3">{{ form_widget(form.comment) }}</td>		
		</tr>
		<tr><td colspan="3">&nbsp;</td></tr>

	</table>
	{%for campus in campuss%}
		<input type="hidden" id="{{campus.reference}}" name="{{campus.reference}}" value="{{campus.address}}"/>
	{%endfor%}
			<th class="sty_label">{{'label.location'|trans([],'BoAdminBundle')}}</th>
			<th class="sty_label">{{'label.campus'|trans([],'BoAdminBundle')}}</th>
			<th class="sty_label">{{'label.preparedby'|trans([],'BoAdminBundle')}}</th>
		</tr>
		<tr>
			<td>{{ form_widget(form.location, {'attr' : {'class' : 'location span3'}}) }}</td>
			<td>{{ form_widget(form.campus, {'attr' : {'class' : 'campus span3'}}) }}</td>
			<td>{{ form_widget(form.preparedby, {'attr' : {'class' : 'preparedby span3'}}) }}</td>

			<th class="sty_label" colspan="2">{{'label.typeofcourse'|trans([],'BoAdminBundle')}}</th>
			<th class="sty_label">{{'label.approvedby'|trans([],'BoAdminBundle')}}</th>
		</tr>
		<tr>
			<td colspan="2">{{ form_widget(form.typeofcourse, {'attr' : {'class' : 'location span3'}}) }}</td>
			<td>{{ form_widget(form.approvedby, {'attr' : {'class' : 'approvedby span3'}}) }}</td>
		</tr>
		<tr><td colspan="3">&nbsp;</td></tr>

{% extends 'base.html.twig' %}
{% block title %}{{parent()}}- {{'layout.menu.dashboard'|trans([],'BoAdminBundle')}} >> {{'entity.timesheet'|trans([],'BoAdminBundle')}} >> {{'action.show'|trans([],'BoAdminBundle')}} >> {{'label.model'|trans([],'BoAdminBundle')}}{% endblock %}
{% block smenu %}{%include "BoAdminBundle:Default:tdb.html.twig"%}{% endblock %}
{% block nav %}	
	{%if employee!=null%}{%include "BoAdminBundle:Employee:rightshow.html.twig"%}{%endif%}	
{% endblock %}
{% block label %}<h1>{{'layout.menu.dashboard'|trans([],'BoAdminBundle')}} >> {{'entity.timesheet'|trans([],'BoAdminBundle')}} >> {{'action.show'|trans([],'BoAdminBundle')}} >> {{'label.model'|trans([],'BoAdminBundle')}}</h1>{% endblock %}	
{% block body %}

	<div id="body-content">
		<form action="{{ path('dash_model_edit',{'id':modelts.id}) }}" method="post" class="style">
			{{ form_errors(model_form) }}
			{%include "BoAdvisorsBundle:Modelts:editactions.html.twig"%}	
			<div class="show_content">
				{%include "BoAdvisorsBundle:Modelts:form.html.twig"%}
			</div>
			{{ form_rest(model_form) }}
		</form>	
	</div>
	
	{% block javascripts %}
		<script type="text/javascript" src="{{ asset('js/vendor/bootstrap/bootstrap.min.js') }}"></script>
		<script type="text/javascript" src="{{ asset('js/vendor/jquery/jquery.min.js') }}"></script>
	{% endblock %}
{% endblock %}
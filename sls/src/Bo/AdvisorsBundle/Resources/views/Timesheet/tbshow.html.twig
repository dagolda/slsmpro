	<table class="show_table">
        <tbody>
            <tr>
				<th>{{'label.week'|trans([],'BoAdminBundle')}}</th>
				<th>{{'label.teacher'|trans([],'BoAdminBundle')}}</th>
				<th>{{'label.studentname'|trans([],'BoAdminBundle')}}</th>
				<th>{{'label.status'|trans([],'BoAdminBundle')}}</th>
            </tr>
            <tr>
                <td>{%if timesheet.tsweek %}{{ timesheet.tsweek.numberweek }} | {{ timesheet.ddate|date('Y') }}{%endif%}</td>
				<td>{%if timesheet.employee %}{{ timesheet.employee.firstname }} {{ timesheet.employee.name }}{%else%}---{%endif%}</td>
				<td>{%if timesheet.group %}{{ timesheet.group.name }}{%elseif timesheet.students%}{{ timesheet.students.firstname }} {{ timesheet.students.name }}{%else%}N/A{%endif%}</td>
				<td>{% if timesheet.status and std[timesheet.status] is defined %}{{std[timesheet.status]}}{%else%}---{%endif%}</td>
            </tr>
            </tr>
            <tr><td colspan="4">&nbsp;</td></tr>
            <tr>
                <th width="20%">{{'label.startam'|trans([],'BoAdminBundle')}}</th>
                <th width="20%">{{'label.endam'|trans([],'BoAdminBundle')}}</th>
				<th width="20%">{{'label.startpm'|trans([],'BoAdminBundle')}}</th>
				<th width="20%">{{'label.endpm'|trans([],'BoAdminBundle')}}</th>
            </tr>
            <tr>                
				<td>{{ timesheet.startam|date('H:i') }}</td>
                <td>{{ timesheet.endam|date('H:i') }}</td>
				<td>{{ timesheet.startpm|date('H:i') }}</td>
				<td>{{ timesheet.endpm|date('H:i') }}</td>
            </tr>
            <tr><td colspan="4">&nbsp;</td></tr>
            <tr>
				<th>{{'label.trainingdate'|trans([],'BoAdminBundle')}}</th>
                <th>{{'label.hour'|trans([],'BoAdminBundle')}}</th>
				<th>Legende</th>
				<th>{{'label.creationdate'|trans([],'BoAdminBundle')}}</th>
            </tr>
            <tr>
                <td>{{ timesheet.ddate|date('Y-m-d H:i:s') }}</td>
                <td>{{ timesheet.hour }} hrs</td>
                <td>{{ timesheet.legende }}</td>
				<td>{% if timesheet.creationdate %}{{ timesheet.creationdate|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr><td colspan="4">&nbsp;</td></tr>
            <tr>
				<th>{{'label.id'|trans([],'BoAdminBundle')}}</th>
				<th>{{'label.category'|trans([],'BoAdminBundle')}}</th>
                <td></td>
				<td></td>
            </tr>
            <tr>
                <td>{{ timesheet.id }}</td>
                <td>{{ timesheet.typets }}</td>
                <td></td>
				<td></td>
            </tr>
            <tr><td colspan="4">&nbsp;</td></tr>
        </tbody>
    </table>
	{%if tsstudents|length>0%}
		<div>
			<legend>Attendance of learners</legend>
			<table border="0" cellpadding="0" cellspacing="0" id="new_row">
				<tr>
					<th>{{'label.firstname'|trans([],'BoAdminBundle')}}</th>
					<th>{{'label.name'|trans([],'BoAdminBundle')}}</th>
					<th>{{'label.legende'|trans([],'BoAdminBundle')}}</th>
					<th>{{'label.delay'|trans([],'BoAdminBundle')}}</th>
					<td width="40%">&nbsp;</td>
				</tr>
				{%for tsstudent in tsstudents%}
					<tr>
						<td class='ts_presence_value'>{{tsstudent.students.firstname}}</td>
						<td class='ts_presence_value'>{{tsstudent.students.name}}</td>
						<td class='ts_presence_value'>{{tsstudent.legende}}</td>
						<td>{{tsstudent.delay}} {{'label.inminutes'|trans([],'BoAdminBundle')}}</td>
						<td>&nbsp;</td>
					</tr>
				{%endfor%}
				<tr><td colspan="4">&nbsp;</td></tr>		
			</table>		
		</div>
	{%endif%}